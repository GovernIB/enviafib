          },
          "200" : {
            "description" : "EFIB: Retornades dades obertes correctament",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/PeticioDeFirmaPaginacio"
                }
              }
            }
          }
        },
        "security" : [ {
          "BasicAuth" : [ ]
        } ]
      }
