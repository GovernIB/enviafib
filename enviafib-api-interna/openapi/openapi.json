  "tags" : [ {
    "name" : "DadesObertesEnviaFib",
    "description" : "Conjunt de mètodes que es poden consultar per obtenir dades obertes"
  } ],
  "paths" : {
    "/secure/dadesobertes/peticionsdefirma" : {
      "get" : {
        "tags" : [ "DadesObertesEnviaFib" ],
        "summary" : "Retorna un llistat amb la informacio de les peticions de firma",
        "operationId" : "peticionsdefirma",
        "parameters" : [ {
          "name" : "startdate",
          "in" : "query",
          "description" : "Data d'inici, en format yyyy-MM-dd (ISO 8601), a partir de la qual volem obtenir dades",
          "schema" : {
            "pattern" : "^(?:[1-9]\\d{3}-(?:(?:0[1-9]|1[0-2])-(?:0[1-9]|1\\d|2[0-8])|(?:0[13-9]|1[0-2])-(?:29|30)|(?:0[13578]|1[02])-31)|(?:[1-9]\\d(?:0[48]|[2468][048]|[13579][26])|(?:[2468][048]|[13579][26])00)-02-29))$",
            "type" : "string"
          },
          "example" : "2022-08-29"
        }, {
          "name" : "enddate",
          "in" : "query",
          "description" : "Data fi, en format yyyy-MM-dd (ISO 8601), fins la qual volem tenir dades",
          "schema" : {
            "pattern" : "^(?:[1-9]\\d{3}-(?:(?:0[1-9]|1[0-2])-(?:0[1-9]|1\\d|2[0-8])|(?:0[13-9]|1[0-2])-(?:29|30)|(?:0[13578]|1[02])-31)|(?:[1-9]\\d(?:0[48]|[2468][048]|[13579][26])|(?:[2468][048]|[13579][26])00)-02-29))$",
            "type" : "string"
          },
          "example" : "2023-12-31"
        }, {
          "name" : "page",
          "in" : "query",
          "description" : "Pàgina de la que es vol obtenir les dades. Comença en 1.",
          "schema" : {
            "type" : "integer",
            "format" : "int32"
          },
          "example" : 1
        }, {
          "name" : "page-size",
          "in" : "query",
          "description" : "Número d'elements a retornar per pàgina. Opcional. Per defecte 10",
          "schema" : {
            "type" : "integer",
            "format" : "int32"
          }
        }, {
          "name" : "language",
          "in" : "query",
          "description" : "Idioma en que s'han de retornar les dades(Només suportat 'ca' o 'es')",
          "schema" : {
            "type" : "string",
            "default" : "ca"
          },
          "examples" : {
            "Català" : {
              "description" : "Català",
              "value" : "ca"
            },
            "Castellano" : {
              "description" : "Castellano",
              "value" : "es"
            }
          }
        } ],
        "responses" : {
          "400" : {
            "description" : "EFIB: Paràmetres incorrectes",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/RestExceptionInfo"
                }
              }
            }
          },
          "401" : {
            "description" : "EFIB: No Autenticat",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "string"
                }
              }
            }
          },
          "403" : {
            "description" : "EFIB: No Autoritzat",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "string"
                }
              }
            }
          },
          "500" : {
            "description" : "EFIB: Error durant la consulta de les dades obertes",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/RestExceptionInfo"
                }
              }
            }
          },
          "200" : {
            "description" : "EFIB: Retornades dades obertes correctament",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/PeticioDeFirmaPaginacio"
                }
              }
            }
          }
        },
        "security" : [ {
          "BasicAuth" : [ ]
        } ]
      }
    },
    "/secure/dadesobertes/tipusdocumentals" : {
      "get" : {
        "tags" : [ "DadesObertesEnviaFib" ],
        "summary" : "Retorna un llistat de tots els tipus documentals",
        "operationId" : "tipusdocumentals",
        "parameters" : [ {
          "name" : "language",
          "in" : "query",
          "description" : "Idioma en que s'han de retornar les dades(Només suportat 'ca' o 'es')",
          "schema" : {
            "type" : "string",
            "default" : "ca"
          },
          "examples" : {
            "Català" : {
              "description" : "Català",
              "value" : "ca"
            },
            "Castellano" : {
              "description" : "Castellano",
              "value" : "es"
            }
          }
        } ],
        "responses" : {
          "401" : {
            "description" : "EFIB: No Autenticat",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "string"
                }
              }
            }
          },
          "403" : {
            "description" : "EFIB: No Autoritzat",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "string"
                }
              }
            }
          },
          "500" : {
            "description" : "EFIB: Error durant la consulta de les dades obertes",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/RestExceptionInfo"
                }
              }
            }
          },
          "200" : {
            "description" : "EFIB: Retornades dades obertes correctament",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/TipusDocumentalsAllElements"
                }
              }
            }
          }
        },
        "security" : [ {
          "BasicAuth" : [ ]
        } ]
      }
    }
  },
  "components" : {
    "schemas" : {
      "RestExceptionInfo" : {
        "required" : [ "code", "errorMessage" ],
        "type" : "object",
        "properties" : {
          "code" : {
            "type" : "integer",
            "description" : "Codi de HTTP de l'error. Veure https://en.wikipedia.org/wiki/List_of_HTTP_status_codes.",
            "format" : "int32"
          },
          "errorMessage" : {
            "type" : "string",
            "description" : "Missatge de l'error"
          },
          "stackTrace" : {
            "type" : "string",
            "description" : "Stacktrace de l'excepció si n'hi hagués."
          },
          "causeException" : {
            "type" : "string",
            "description" : "Tipus de l'excepció origen (cause) si n'hi hagués."
          },
          "causeStackTrace" : {
            "type" : "string",
            "description" : "Stacktrace de l'excepció origen (cause) si n'hi hagués."
          }
        },
        "description" : "Estructura de dades utilitzada per passar informació d'un error"
      },
      "PeticioDeFirma" : {
        "type" : "object",
        "properties" : {
          "nif" : {
            "type" : "string"
          },
          "titol" : {
            "type" : "string"
          },
          "creada" : {
            "type" : "string",
            "format" : "date-time"
          },
          "finalitzada" : {
            "type" : "string",
            "format" : "date-time"
          },
          "idiomaCode" : {
            "type" : "string"
          },
          "idiomaDescription" : {
            "type" : "string"
          },
          "tipusDocumentalCode" : {
            "type" : "string"
          },
          "tipusDocumentalDescription" : {
            "type" : "string"
          },
          "tipusPeticioCode" : {
            "type" : "integer",
            "format" : "int32"
          },
          "tipusPeticioDescription" : {
            "type" : "string"
          },
          "dir3" : {
            "type" : "string"
          }
        },
        "description" : "Elements retornats. Pot retornar un null o una llista buida si no hi ha elements."
      },
      "PeticioDeFirmaPaginacio" : {
        "required" : [ "date-download", "items-returned", "name", "page", "page-size", "total-count", "total-pages" ],
        "type" : "object",
        "properties" : {
          "name" : {
            "type" : "string",
            "description" : "Nom descriptiu del que s'està retornant"
          },
          "page" : {
            "type" : "integer",
            "description" : "Número pàgina. Comença per 1.",
            "format" : "int32"
          },
          "page-size" : {
            "type" : "integer",
            "description" : "Mida de pàgina",
            "format" : "int32"
          },
          "total-pages" : {
            "type" : "integer",
            "description" : "Número total de pàgines",
            "format" : "int32"
          },
          "total-count" : {
            "type" : "integer",
            "description" : "Número total d'elements",
            "format" : "int32"
          },
          "items-returned" : {
            "type" : "integer",
            "description" : "Numero d'elements retornats",
            "format" : "int32"
          },
          "next" : {
            "type" : "string",
            "description" : "Si hi ha més elements, llavors retorna la URL a la següent pàgina de dades."
          },
          "date-download" : {
            "pattern" : "^(?:[1-9]\\d{3}-(?:(?:0[1-9]|1[0-2])-(?:0[1-9]|1\\d|2[0-8])|(?:0[13-9]|1[0-2])-(?:29|30)|(?:0[13578]|1[02])-31)|(?:[1-9]\\d(?:0[48]|[2468][048]|[13579][26])|(?:[2468][048]|[13579][26])00)-02-29)T(?:[01]\\d|2[0-3]):[0-5]\\d:[0-5]\\d(?:Z|[+-][01]\\d:[0-5]\\d)$",
            "type" : "string",
            "description" : "Data i hora en que s'han retornat les dades. Format ISO-8601"
          },
          "data" : {
            "type" : "array",
            "description" : "Elements retornats. Pot retornar un null o una llista buida si no hi ha elements.",
            "items" : {
              "$ref" : "#/components/schemas/PeticioDeFirma"
            }
          }
        }
      },
      "TipusDocumental" : {
        "required" : [ "code", "name" ],
        "type" : "object",
        "properties" : {
          "code" : {
            "type" : "string",
            "description" : "Identificador intern de l'objecte"
          },
          "name" : {
            "type" : "string",
            "description" : "Nom del Tipus Documental"
          }
        },
        "description" : "Informació bàsica d'un Tipus Documental"
      },
      "TipusDocumentalsAllElements" : {
        "required" : [ "date-download", "name", "total-count" ],
        "type" : "object",
        "properties" : {
          "name" : {
            "type" : "string",
            "description" : "Nom descriptiu del que s'està retornant"
          },
          "total-count" : {
            "type" : "integer",
            "description" : "Número total d'elements",
            "format" : "int32"
          },
          "date-download" : {
            "pattern" : "^(?:[1-9]\\d{3}-(?:(?:0[1-9]|1[0-2])-(?:0[1-9]|1\\d|2[0-8])|(?:0[13-9]|1[0-2])-(?:29|30)|(?:0[13578]|1[02])-31)|(?:[1-9]\\d(?:0[48]|[2468][048]|[13579][26])|(?:[2468][048]|[13579][26])00)-02-29)T(?:[01]\\d|2[0-3]):[0-5]\\d:[0-5]\\d(?:Z|[+-][01]\\d:[0-5]\\d)$",
            "type" : "string",
            "description" : "Data i hora en que s'han retornat les dades. Format ISO-8601"
          },
          "data" : {
            "type" : "array",
            "description" : "Elements retornats. Pot retornar un null o una llista buida si no hi ha elements.",
            "items" : {
              "$ref" : "#/components/schemas/TipusDocumental"
            }
          }
        }
      }
    },
    "securitySchemes" : {
      "BasicAuth" : {
        "type" : "http",
        "scheme" : "basic"
      }
    }
  }
}